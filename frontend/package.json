{
  "name": "gravity-ui-example-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/view": "^6.36.5",
    "@gravity-ui/date-components": "^3.0.2",
    "@gravity-ui/date-utils": "^2.5.6",
    "@gravity-ui/icons": "^2.13.0",
    "@gravity-ui/navigation": "^3.1.1",
    "@gravity-ui/table": "^1.9.0",
    "@gravity-ui/uikit": "^7.4.0",
    "@react-awesome-query-builder/core": "6.2.0",
    "@tanstack/react-query": "^5.68.0",
    "@tanstack/react-query-devtools": "^5.68.0",
    "@uiw/codemirror-theme-bbedit": "^4.23.10",
    "@uiw/react-codemirror": "^4.23.10",
    "@uiw/react-markdown-editor": "^6.1.2",
    "@uiw/react-markdown-preview": "^5.1.3",
    "@uiw/react-md-editor": "^4.0.5",
    "axios": "^1.8.3",
    "bem-cn-lite": "^4.1.0",
    "chart.js": "^4.4.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.5",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
    "immutable": "^3.8.2",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "marked": "^15.0.8",
    "next": "^15.2.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-collapsed": "^4.2.0",
    "react-dom": "^18.3.1",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.3.0",
    "@gravity-ui/eslint-config": "^3.2.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@next/eslint-plugin-next": "^15.2.2",
    "@svgr/webpack": "^8.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "stylelint": "^15.11.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
